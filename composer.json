{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "doctrine/dbal": "^2.13",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "barryvdh/laravel-async-queue": "v0.7.4",
        "bugsnag/bugsnag-laravel": "^2.22",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "dimsav/laravel-translatable": "^9.3",
        "laravel/framework": "^8.83",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1.8",
        "league/oauth2-client": "^2.5",
        "renatomarinho/laravel-page-speed": "^1.9",
        "overtrue/laravel-lang": "^3.0",
        "spatie/laravel-activitylog": "^4.1",
        "spatie/laravel-medialibrary": "^9.12",
        "yajra/laravel-datatables-oracle": "^9.11"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "kkomelin/laravel-translatable-string-exporter": "^1.13",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10",
        "fakerphp/faker": "^1.9.1",
        "facade/ignition": "^2.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
